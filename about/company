<script>
    function addUserIdentity(){
        if (typeof mixpanel === "undefined") return;

        var userCookies = getCookie("mixpanel_user_cookies")
        if (!userCookies){
            userCookies = Math.random().toString(36).slice(2)
            setCookie("mixpanel_user_cookies",userCookies)
        }
        mixpanel.identify(userCookies);            
        mixpanel.people.set_once({
                "$distinct_id": userCookies,
                "$name": userCookies
        });

    }
    addUserIdentity()
